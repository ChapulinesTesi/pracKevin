def suma(n1=0,n2=0):
    return n1+n2
def SUMAR(funcion,n1=0,n2=0):
    return funcion(n1,n2)

def resta(n1=0,n2=0):
    return n1-n2
def RESTAR(funcion,n1=0,n2=0):
    return funcion(n1,n2)
    
print("PROGRAMA QUE REALIZA OPERACIONES CON 2 NÃšMEROS\n")
numero1=float (input("Ingresa el primer numero: "))
numero2=float (input("Ingresa el segundo numero: "))

print("\n RESULTADOS DE OPERACION")
fun_suma=suma
ress=SUMAR(fun_suma,numero1,numero2)
print("La suma es =",ress)

fun_resta=resta
resr=RESTAR(fun_resta,numero1,numero2)
print("La resta es =",resr)
